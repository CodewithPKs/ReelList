{
  "cli": {
    "version": ">= 3.12.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_XCODE_OTHER_CPLUSPLUSFLAGS": "-D_LIBCPP_ENABLE_CXX17_REMOVED_UNARY_BINARY_FUNCTION"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
